{
  "compilerOptions": {
    "outDir": "./dist/",
    "moduleResolution": "node",
    "baseUrl": "./src/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "skipLibCheck": true,
    "paths": {
        "web": [
            "./src/web/*"
        ]
    }
  }
}
